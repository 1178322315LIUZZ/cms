<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.zhenzhen.cms.dao.ArticleDao">
  	<insert id="insert">
  insert into 
  cms_article(title,content,summary,user_id,channel_id,category_id,created,updated,hits,hot,deleted,status,content_type,picture)
  values(#{title},#{content},#{summary},#{userId},#{channelId},#{categoryId},
  #{created},#{updated},#{hits},#{hot},#{deleted},#{status},#{contentType},#{picture})
  	</insert>
 <select id="selects" resultType="Article">
   select * from cms_article
 </select>
  </mapper>